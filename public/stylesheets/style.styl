.desc
	text-align center
	color #777
	font-size 15px
	text-shadow 0 2px 0 white

.usage
	margin-top	10px
	text-align center
	color #aaa
	font-size 13px
	text-shadow 0 2px 0 white

.commonbg
	background url('/images/lightbg_3.png') repeat top center
	padding-bottom 30px

#navigationbar
	position fixed
	overflow auto
	top 0
	left 5%
	width 200px
	background #fff
	-moz-box-shadow  0 -3px 3px #aaa inset
	-webkit-box-shadow  0 -3px 3px #aaa inset
	box-shadow 0 -3px 3px #aaa inset
	z-index 200
	border-left 1px solid #aaa
	border-bottom 1px solid #aaa
	border-right 1px solid #aaa
	border-bottom-right-radius 5px
	border-bottom-left-radius 5px
	border-top none
#logo_img
	-moz-box-shadow  0 2px 2px rgba(0,0,0,0.5) 
    -webkit-box-shadow 0 2px 2px rgba(0,0,0,0.5) 
    box-shadow  0 2px 2px rgba(0,0,0,0.5) 

#navigationlist
	padding 20px
#navigationbar ul
	padding 5px
	margin 0
.navigationitem
	a
		display block
		padding 2px 10px
		line-height 20px
		color #999
		word-break break-all
		word-wrap break-word
		cursor pointer
	a:hover
		color #666

#content
	position fixed
	overflow auto
	top 0
	bottom 0
	left 200px
	right 0
	bottom 0
	padding-top 30px
	
	

.doc_header
	position relative
	margin 30px auto 0 auto
	width 600px
	
.doc
	position relative
	margin 0 30px 30px 30px
	padding 15px
	
	word-break break-all
	word-wrap break-word
.art_author 
	color #999
	display block
	float right
