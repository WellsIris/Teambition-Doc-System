.cap_code
	position relative
	width 98% 
	margin 20px auto
	background-color #fff
	overflow-y hidden
	border  1px solid #e0e0e0
	border-radius 5px
	box-shadow 0 1px 2px #aaa

.sharehandler
	position absolute
	top 0
	right -55px
	font-family texticon
	font-size 50px
	color rgba(0,0,0,0.3)
	text-shadow 1px 1px 0px rgba(255,255,255,0.5)
	content '/'

#editor
	position relative
	width 80%
	min-width 600px
	max-width 800px
	min-height 100%
	margin 0 auto
	background-color #fff
	border-top 1px solid #fff
	border-top-left-radius 5px
