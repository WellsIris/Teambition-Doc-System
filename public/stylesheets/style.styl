@font-face
	font-family texticon
	src: url('texticon.eot'); /* IE9*/
	src: url('texticon.eot?#iefix') format('embedded-opentype'),url('texticon.woff') format('woff'),url('texticon.ttf')  format('truetype'),url('texticon.svg#svgFontName') format('svg');

.texticon
	font-family texticon


h1
	text-align center
	line-height 70px
	font-size 21px
	font-weight bold
	color #AAA
	text-shadow 0 2px 0 white

h2
	font-size 30px
	color #666
	text-align center
	font-weight bold
	margin-bottom 5px
	margin-top 10px
	cursor pointer
h3
	font-size 21px
	color #02b6d6
	font-weight bold
	margin 10px


h4
	font-size 16px
	color #666
	margin 2px
h5
	font-size 16px
	color #666
	margin 5px auto 0 10px
body
	background url('/images/lightbg_3.png') repeat
	font-family helvetica neue,helvetica,arial,sans-serif
	color #434343	
._blue
	color #02b6d6
.h10 
	height 10px
